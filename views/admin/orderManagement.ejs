          <!-- card-header end// -->
          <div class="card-body">
            <div class="table-responsive">
              <%if(orders.length > 0){%>
              <table class="table table-hover">
                <thead>
                  <tr>
                    <th class="text-center">Si No.</th>
                    <th class="text-center">Order ID</th>
                    <th class="text-center" scope="col">Name</th>
                    <th class="text-center" scope="col">Total Amount</th>
                    <th class="text-center" scope="col">Order Status</th>
                    <th class="text-center" scope="col">Payment Status</th>
                    <th class="text-center" scope="col">Date</th>
                    <th class="text-center" scope="col" class="text-end">
                      Action
                    </th>
                  </tr>
                </thead>
                <%orders.reverse().forEach((order,index) => {%>
                <tbody>
                  <tr>
                    <td class="text-center"><%= index+1%></td>
                    <td class="text-center"><%=order.referenceId%></td>
                    <td class="text-center">
                      <b><%= order.userId.userName %></b>
                    </td>
                    <td class="text-center">₹<%=order.totalAmount%></td>
                    <td class="text-center">
                      <span
                        class="badge rounded-pill alert-<%= order.orderStatus === 'Order Placed' ? 'danger' : order.orderStatus === 'Shipped' ? 'warning' : order.orderStatus === 'Delivered' ? 'success' : order.orderStatus === 'Cancelled' ? 'danger' : order.orderStatus === 'Returned'? 'secondary' : order.orderStatus ==='Order Failed'?'dark':'' %>"
                        ><%=order.orderStatus%>
                      </span>
                    </td>
                    <td class="text-center">
                      <span
                        class="badge rounded-pill alert-<%= order.paymentStatus === 'Success' ? 'success' : order.paymentStatus === 'Pending' ? 'warning' : order.paymentStatus === 'Failed' ? 'danger' : '' %>"
                      >
                        <%=order.paymentStatus%>
                      </span>
                    </td>
                    <td class="text-center">
                      <%=order.orderDate.toLocaleDateString()%>
                    </td>
                    <td class="text-center">
                      <a
                        href="/admin/order/details-page/<%=order._id%>"
                        class="btn btn-md rounded font-sm"
                        >Details</a
                      >
                    </td>
                  </tr>
                </tbody>
                <%})%>
              </table>
              <%}else{%>
              <div class="container">
                <div
                  class="text-danger d-flex align-items-center justify-content-center bg-secondary text-light p-4"
                >
                  <div class="bg-white p-10">
                    <h4 class="text-danger">Sorry,No Orders Found Yet!!</h4>
                  </div>
                </div>
              </div>
              <%}%>
            </div>
            <!-- table-responsive //end -->
