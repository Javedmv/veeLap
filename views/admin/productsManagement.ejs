      <section class="content-main">
        <div class="content-header">
          <div>
            <h2 class="content-title card-title">Products List</h2>
            <p>List of all the products.</p>
          </div>
          <div>
            <!-- <a href="#" class="btn btn-light rounded font-md">Export</a>
            <a href="#" class="btn btn-light rounded font-md">Import</a> -->
            <a
              href="/admin/view-add-product"
              class="btn btn-primary btn-sm rounded"
              >Create new</a
            >
          </div>
        </div>
        <div class="card mb-4">
          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-bordered table-striped">
                <thead>
                  <tr>
                    <th class="text-center col-1">Si.no</th>
                    <th class="text-center">Image</th>
                    <th class="text-center">Product Name</th>
                    <th class="text-center">Brand</th>
                    <th class="text-center">Category</th>
                    <!-- <th class="text-center col-2">Description</th> -->
                    <th class="text-center">Stock</th>
                    <th class="text-center">Regular Price</th>
                    <th class="text-center">Sales Price</th>
                    <th class="text-center">Status</th>
                    <th class="text-center">Action</th>
                    <!-- Adding an extra column for the action buttons -->
                  </tr>
                </thead>
                <tbody>
                  <% let i=1; %> <% product.reverse().forEach((e)=>{ %>
                  <tr>
                    <td>
                      <div class="form-check text-center"><%=i++ %></div>
                    </td>
                    <td>
                      <div class="form-check text-center">
                        <% if (e && e.images && e.images.length > 0 &&
                        e.images[0].path) { %>
                        <img src="<%= e.images[0].path %>" alt="" />
                        <% } else { %>
                        <!-- Handle the case when the image path is not available or valid -->
                        <span>No Image Available</span>
                        <% } %>
                      </div>
                    </td>

                    <td>
                      <div class="form-check text-center">
                        <%=e.productName %>
                      </div>
                    </td>
                    <td>
                      <div class="form-check text-center"><%=e.brand %></div>
                    </td>
                    <td>
                      <div class="form-check text-center">
                        <%=e.category.categoryName %>
                      </div>
                    </td>
                    <!-- <td>
                      <div class="form-check  text-center"></div>
                    </td> -->
                    <td>
                      <div class="form-check text-center"><%=e.stock %></div>
                    </td>
                    <td>
                      <div class="form-check text-center">
                        <%=e.regularPrice %>
                      </div>
                    </td>
                    <td>
                      <div class="form-check text-center">
                        <%=e.salesPrice %>
                      </div>
                    </td>
                    <td>
                      <div
                        class="form-check text-center rounded-pill alert-<%= (e.status === 'Active') ? 'success' : 'danger' %>"
                      >
                        <%= e.status %>
                      </div>
                    </td>
                    <td>
                      <div class="form-check d-flex">
                        <a
                          href="/admin/edit-product/<%=e._id%>"
                          class="btn-sm btn-secondary mr-5"
                          >Edit</a
                        >
                        <a
                          href="/admin/delete-product/<%=e.id%>"
                          class="btn-sm btn-danger"
                          >Delete</a
                        >
                      </div>
                    </td>
                  </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
          </div>
        </div>

