    <script>
      document.addEventListener("DOMContentLoaded",function(){
        document.getElementById("editAccountBtn").addEventListener("click",function(){
          const actDetailsPage = document.getElementById("accountDetails")
          const editPage = document.getElementById("editDetails")
          actDetailsPage.style.display = "none"
          editPage.style.display = "block"
