      </div>
      <section class="pt-50 pb-50">
        <div class="container">
          <div class="row">
            <div class="col-lg-10 m-auto">
              <div class="row">
                <div class="col-md-4">
                  <div class="dashboard-menu">
                    <ul class="nav flex-column" role="tablist">
                      <li class="nav-item">
                        <a
                          class="nav-link active"
                          id="dashboard-tab"
                          data-bs-toggle="tab"
                          href="#dashboard"
                          role="tab"
                          aria-controls="dashboard"
                          aria-selected="false"
                          ><i class="fi-rs-settings-sliders mr-10"></i
                          >Dashboard</a
                        >
                      </li>
                      <li class="nav-item">
                        <a
                          class="nav-link"
                          id="orders-tab"
                          data-bs-toggle="tab"
                          href="#orders"
                          role="tab"
                          aria-controls="orders"
                          aria-selected="false"
                          ><i class="fi-rs-shopping-bag mr-10"></i>Orders</a
                        >
                      </li>

                      <li class="nav-item">
                        <a
                          class="nav-link"
                          id="address-tab"
                          data-bs-toggle="tab"
                          href="#address"
                          role="tab"
                          aria-controls="address"
                          aria-selected="true"
                          ><i class="fi-rs-marker mr-10"></i>My Address</a
                        >
                      </li>
                      <li class="nav-item">
                        <a
                          class="nav-link"
                          id="account-detail-tab"
                          data-bs-toggle="tab"
                          href="#account-detail"
                          role="tab"
                          aria-controls="account-detail"
                          aria-selected="true"
                          ><i class="fi-rs-user mr-10"></i>Account details</a
                        >
                      </li>
                      <li class="nav-item">
                        <a
                          class="nav-link"
                          id="wallet"
                          data-bs-toggle="tab"
                          href="#wallet-details"
                          role="tab"
                          aria-controls="wallet"
                          aria-selected="true"
                          ><i class="fi-rs-user mr-10"></i>Wallet</a
                        >
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="/logout"
                          ><i class="fi-rs-sign-out mr-10"></i>Logout</a
                        >
                      </li>
                    </ul>
                  </div>
                </div>
                <div class="col-md-8">
                  <div class="tab-content dashboard-content">
                    <div
                      class="tab-pane fade active show"
                      id="dashboard"
                      role="tabpanel"
                      aria-labelledby="dashboard-tab"
                    >
                      <div class="card">
                        <div class="card-header">
                          <h5 class="mb-0">Hello <%= user.userName%>!</h5>
                        </div>
                        <div class="card-body">
                          <p>
                            From your account dashboard. you can easily check
                            &amp; view your <a href="#">recent orders</a>,
                            manage your
                            <a href="#">shipping and billing addresses</a> and
                            <a href="#"
                              >edit your password and account details.</a
                            >
                          </p>
                        </div>
                      </div>
                    </div>
                    <div
                      class="tab-pane fade"
                      id="orders"
                      role="tabpanel"
                      aria-labelledby="orders-tab"
                    >
                      <div class="card">
                        <div class="card-header">
                          <h5 class="mb-0">Your Orders</h5>
                        </div>
                        <%if(orders){%>
                        <div class="card-body">
                          <div class="table-responsive">
                            <table class="table">
                              <thead>
                                <tr>
                                  <th>Si no</th>
                                  <th>Image</th>
                                  <th>Product Name</th>
                                  <th>Date</th>
                                  <th>Order Status</th>
                                  <th>Total</th>
                                  <th>Actions</th>
                                </tr>
                              </thead>
                              
                              <tbody>
                                <% if (orders && orders.length > 0) { %>
                                  <% let previousOrderIndex = null; %>
                                  <% orders.reverse().forEach((order, productIndex) => { %>
                                    <% order.products.forEach((product, productInnerIndex) => { %> 
                                      <tr>
                                        <% if (previousOrderIndex !== productIndex) { %>
                                          <td rowspan="<%= order.products.length %>" class="text-center"><%= productIndex + 1 %></td>
                                          <% previousOrderIndex = productIndex; %>
                                        <% } %>
                                        <td style="width: 2rem; height: 2rem;"><img src="<%= product?.productId?.images[0]?.path %>" alt="image"></td>
                                        <td class="text-center"><%= product?.productId?.productName %></td>
                                        <td class="text-center"><%= order.orderDate.toLocaleDateString() %></td>
                                        <% if (productInnerIndex === 0) { %>
                                        <td class="text-center"  rowspan="<%= order.products.length %>">
                                          <span class="badge rounded-pill alert-<%= order.orderStatus === 'Order Placed' ? 'info' : order.orderStatus === 'Shipped' ? 'warning' : order.orderStatus === 'Delivered' ? 'success' : order.orderStatus === 'Cancelled' ? 'danger' : order.orderStatus === 'Returned'? 'secondary' : order.orderStatus === 'Order Failed'? 'dark' : '' %>">
                                            <%=order.orderStatus%>
                                          </span>
                                        </td>
                                        <% } %>
                                        <td class="text-center"> ₹<%= product.price %> for <%= product.quantity %> item</td>
                                        <% if (productInnerIndex === 0) { %>
                                          <td class="text-center" rowspan="<%= order.products.length %>">
                                            <a href="/order-status-details?orderRefId=<%= order.referenceId %>" class="btn btn-small d-block">View</a>
                                          </td>
                                        <% } %>
                                      </tr>
                                    <% }); %>
                                    <tr>
                                      <td colspan="7"><hr></td>
                                    </tr>
                                  <% }); %>
                                <% } %>
                              </tbody>
                            </table>
                          </div>
                        </div>
                        <%}else{%>
                          <div class="container">
                            <div class="text-danger d-flex align-items-center justify-content-center bg-secondary text-light p-4">
                              <div class="bg-white p-10">
                               <h4 class="text-primary"> No order available yet</h4>
                              </div>
                            </div>
                          </div>
                          <%}%>
                      </div>
                    </div>
                    <div
                    class="tab-pane fade"
                    id="address"
                    role="tabpanel"
                    aria-labelledby="address-tab"
                    >
                    
                    <div class="row">
                      <%if(address){%>
                    <% address.address.forEach((add,index) => {%>
                        <div class="col-lg-6 d-block align-items-spacebetween justify-content-spacebetween ">
                          <div class="card mb-3 mb-lg-0 "<% if (index === 2) { %>style="margin-top: 20px;"<% } %>>
                            <div class="card-header">
                              <h5 class="mb-0"><%=add.addressType %></h5>
                            </div>
                            <div class="card-body">
                              <address>
                                  <strong>Name:</strong> <%=add.name%><br>
                                  <strong>City:</strong> <%=add.city%>,<br>
                                  <strong>State: </strong> <%=add.state%>
                                  <strong>Landmark:</strong> <%=add.landMark%><br>
                                  <strong>Pincode:</strong> <%=add.pincode%><br>
                              </address>
                              <p><strong>Phone:</strong> <%=add.phone%></p>
                              <div class="d-flex align-items-center justify-content-between">
                                <a href="/edit-address/<%=add._id%>" class="btn btn-primary btn-small" style="background-color: rgb(67, 66, 66);border:rgb(69, 69, 69) ;">Edit</a>
                                <a href="/delete-single-address/<%=add._id%>" class=" btn btn-small" style="background-color: rgb(230, 0, 0);border:rgb(90, 25, 25) ;">Delete</a>
                              </div>
                          </div>
                          
                          </div>
                        </div>
                        <%  }) %>
                        <%}else{%>
                          <div class="container">
                            <div class="text-danger d-flex align-items-center justify-content-center bg-secondary text-light p-4">
                              <div class="bg-white p-10">
                               <h4 class="text-danger"> Sorry no address found</h4>
                              </div>
                            </div>
                          </div>
                          <%}%>
                        
                        <div class="d-flex align-items-center justify-content-end mt-20">
                          <a href="/add-address" class="btn sm-btn">
                            Add Address
                          </a>
                        </div>
                      </div>   
                    </div>
                    <div class="tab-pane fade" id="account-detail" role="tabpanel" aria-labelledby="account-detail-tab">
                      <div id="accountDetails">
                        <div class="card col-lg-10 mx-auto">
                          <div class="card-header text-center">
                              <h5 class="d-block align-items-center">Account Details</h5>
                            </div>
                            <div class="card-body">
                           <p><strongs>User Name :</strongs><%=user.userName%> </p>  
                           <p><strongs>Email :</strongs><%=user.email%> </p> 
                           <p><strongs>Phone :</strongs><%=user.phoneNumber%> </p>
                           <div class="text-center">
                            <button type="button" id="editAccountBtn" class="btn btn-primary btn-sm">Edit Account</button>
                        </div>
                            </div>
                          </div>
                          <div class="d-flex align-items-center justify-content-end mr-60 mt-5">
                            <a href="/load-forgot-pass" class="text-primary">Forgot Password?</a>
                          </div>
                      <div class=" col-lg-10 mx-auto d-flex">
                        <div class="mr-20">
                          Referral Code : <%=user.ReferralCode%>
                        </div>
                      </div>
                    </div>
                    

                    <div id="editDetails" style="display: none;">
                      <div class="card col-lg-10 mx-auto">
                            <div class="card-header text-center">
                                <h5 class="d-block align-items-center">Edit Account Details</h5>
                            </div>
                            <div class="card-body">
                                <form method="post" action="/edit-user-details">
                                    <div class="mb-3">
                                        <label for="newUserName" class="form-label"><strong>User Name:</strong></label>
                                        <input type="text" class="form-control " id="newUserName" name="newUserName" placeholder="Enter new user name" value="<%= user.userName%>">
                                    </div>
                    
                                    <div class="mb-3">
                                        <label for="newEmail" class="form-label"><strong>Email:</strong></label>
                                        <input type="email" class="form-control" id="newEmail" name="newEmail" placeholder="Enter new email" value="<%= user.email%>">
                                    </div>
                    
                                    <div class="mb-3">
                                        <label for="newPhoneNumber" class="form-label"><strong>Phone Number:</strong></label>
                                        <input type="tel" class="form-control" id="newPhoneNumber" name="newPhoneNumber" placeholder="Enter new phone number" value="<%= user.phoneNumber%>">
                                    </div>
                    
                                    <div class="text-center d-flex justify-content-between">
                                      <a class="btn btn-sm" href="/profile" style="background-color: rgb(69, 69, 69)
                                      ;">Cancel</a>
                                      <button type="submit" class="btn btn-primary btn-sm edit-user-save">Save Changes</button>
                                    </div>
                                </form>
                              </div>
                            </div>
                          </div>
                        </div> 
                        <div class="tab-pane fade" id="wallet-details" role="tabpanel" aria-labelledby="wallet-details">
                          <div id="walletDetails ">
                            <div class="card  mx-auto">
                                <div class="card-header text-center">
                                  <h5 class="d-block align-items-center">Wallet</h5>
                                </div>
                                <div class="card-body">
                                  <div class="" style="font-size: large;"></span>
                                    Balance: ₹<span><%=wallet.balance%></span>
                                 <p>your wallet balance can be redeemed on your next order</p>
                                  </div>
                                </div>
                            </div>
                          </div>
                          <% if (wallet.historyDetails) { %>
                            <table>
                              <tr>
                                <th class="text-center">Si no</th>
                                <th class="text-center">Referance Id</th>
                                <th class="text-center">Status</th>
                                <th class="text-center">Amount</th>
                                <th class="text-center">Current Balance</th>
                                <th class="text-center">Date & Time</th>
                              </tr>
                            <%
                            let sino = 1;
                            wallet.historyDetails.reverse().forEach((element) =>{
                            %>
                           
                            <tr>
                              <td class="text-center"><%= sino++%></td>
                              <td class="text-center"><a href="/order-status-details?orderRefId=<%= element.referenceId %>"><%=element.referenceId%></a></td>
                              <td class="text-center"><%=element.walletStatus%></td>
                              <%if(element.walletStatus === "Debited"){%>
                              <td class="text-center text-danger">-<%=element.orderTotal%></td>
                              <%}else{%>
                              <td class="text-center text-success">+<%=element.orderTotal%></td>
                                <%}%>
                              <td class="text-center text-success">₹<%=element.walletBalance%></td>
                              <td class="text-center"><%=element.time%></td>
                            </tr>
                            <%})%>
                          </table>
                          <%}%>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
